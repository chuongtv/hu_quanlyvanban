<div class="row">
    @*các thao tác*@
    <div class="show">
        <div class="col-sm-4 col-sm-push-8" style="text-align:right;">
            <div class="form-group ">
                <button data-otf-mutil="true"
                        onclick="QuanLySendEmails_MyEmailConfig_DeleteMulti(this)"
                        data-otf-confirm="Bạn có chắc muốn xóa những dòng đã chọn?"
                        data-otf-target="#QuanLySendEmails_MyEmailConfig_DS_content"
                        data-otf-input="#QuanLySendEmails_MyEmailConfig_SearchInput"
                        data-otf-action-target="@Url.Action("MyEmailConfigGet_List", "QuanLyMaketting")?k="
                        data-otf-action="@Url.Action("MyEmailConfigDelete", "QuanLyMaketting", null, Request.Url.Scheme)?id="
                        class=" btn btn-danger"
                        title="Xóa">
                    <i class="fa fa-trash-o"></i> Xóa
                </button>
                <a data-toggle="modal" onclick="QuanLySendEmails_MyEmailConfig_GetView(this)"
                   data-target="#modalQuanLySendEmails_MyEmailConfig" class=" btn btn-success" data-otf-action="@Url.Action("MyEmailConfigCreate", "QuanLyMaketting", null, Request.Url.Scheme)?id=@Request["id"]" href="javascript:void(0)">
                    <i class="fa fa-plus" title="Tạo mới"></i> Thêm mới
                </a>
                

            </div>
        </div>
        <div class="col-sm-8  col-sm-pull-4">
            
        </div>
    </div>
</div>
<div class="row">
    <div class="col-sm-12" id="QuanLySendEmails_MyEmailConfig_DS_content">
        @Html.Action("MyEmailConfigGet_List", "QuanLyMaketting")
    </div>
</div>

