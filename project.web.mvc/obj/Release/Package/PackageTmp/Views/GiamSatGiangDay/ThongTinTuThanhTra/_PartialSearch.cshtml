{
    <label class="control-label col-sm-1 linelable" style="float:left;">Đon vị</label>
        <div class="col-sm-2">
            @Html.DropDownListFor(model => Model.DonVi_Guid, new SelectList(Model.ListDonVi_Guid, "ValueString", "Name"), new { @class = "form-control chosen-select", id = "DDL_DonVi_Guid", @onchange = "FillListGiangVien()" })
            @Html.ValidationMessageFor(model => Model.DonVi_Guid, null, new { @class = "text-danger" })
        </div>
    }
    @if ((@User as project.web.mvc.Common.IOIORTPrincipal).HasPermission("PhieuMuonPhongCreate_GiamSatGiangDay") // Quyen thu ky
