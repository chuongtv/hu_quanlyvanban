{
    <label class="control-label col-sm-1 linelable" style="float:left;">Giảng viên</label>
        <div class="col-sm-2 form-group ">
            @Html.DropDownListFor(model => Model.NhanVien_Guid, new SelectList(Model.ListNhanVien_Guid, "ValueString", "Name"), new { @class = "form-control chosen-select", id = "DDL_NhanVien_Guid" })
            @Html.ValidationMessageFor(model => Model.NhanVien_Guid, null, new { @class = "text-danger" })
        </div>
    }

</div>

